<?php

{% if header %}{{ header }}{% endif %}

namespace {{ class.namespace }};

{% for use in class.uses %}
use {{ use }};
{% endfor %}

/**
{% for annotation in class.annotations %}
 * {{ annotation }}
{% endfor %}
 */
class {{ class.name }}{% if class.parent %} extends {{ class.parent }}{% endif %}{% if class.interfaceName %} implements {{ class.interfaceName }}{% endif %}

{
{% for constant in class.constants %}
    /**
{% for annotation in constant.annotations %}
     * {{ annotation }}
{% endfor %}
    */
    const {{ constant.name }} = '{{ constant.value }}';
{% endfor %}
{% for field in class.fields %}
    /**
{% for annotation in field.annotations %}
     * {{ annotation }}
{% endfor %}
     */
    {{ fieldVisibility }} ${{ field.name }};
{% endfor %}
{% for field in class.fields %}

    /**
{% for annotation in field.setterAnnotations %}
     * {{ annotation }}
{% endfor %}
     */
    public function set{{ field.name|ucfirst }}({% if field.typeHint %}{{ field.typeHint }} {% endif %}${{ field.name }})
    {
        $this->{{ field.name }} = ${{ field.name }};

        return $this;
    }

    /**
{% for annotation in field.getterAnnotations %}
    * {{ annotation }}
{% endfor %}
    */
    public function get{{ field.name|ucfirst }}()
    {
        return $this->{{ field.name }};
    }
{% endfor %}
}
